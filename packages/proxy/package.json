{
  "name": "@braintrust/proxy",
  "version": "0.0.4",
  "description": "A proxy server that load balances across AI providers.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -r dist/*",
    "proto": "./node_modules/.bin/pbjs -t static-module -o prom/prom.js prom/prom.proto && ./node_modules/.bin/pbts -o prom/prom.d.ts prom/prom.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./edge": {
      "types": "./edge/dist/index.d.ts",
      "import": "./edge/dist/index.mjs",
      "module": "./edge/dist/index.mjs",
      "require": "./edge/dist/index.js"
    },
    "./schema": {
      "types": "./schema/dist/index.d.ts",
      "import": "./schema/dist/index.mjs",
      "module": "./schema/dist/index.mjs",
      "require": "./schema/dist/index.js"
    },
    "./prom": {
      "types": "./prom/dist/index.d.ts",
      "import": "./prom/dist/index.mjs",
      "module": "./prom/dist/index.mjs",
      "require": "./prom/dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "edge/dist/**/*",
    "schema/dist/**/*",
    "prom/dist/**/*"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://www.braintrustdata.com/docs/guides/proxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintrustdata/braintrust-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/braintrustdata/braintrust-proxy/issues"
  },
  "keywords": [
    "ai",
    "proxy",
    "vercel",
    "cloudflare",
    "workers",
    "edge",
    "openai",
    "lambda",
    "express"
  ],
  "devDependencies": {
    "@types/node": "^20.9.4",
    "esbuild": "^0.19.7",
    "long": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "protobufjs-cli": "^1.1.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "eventsource-parser": "^1.1.1",
    "openai": "^4.19.1",
    "protobufjs": "^7.2.5",
    "snappyjs": "^0.7.0"
  }
}
